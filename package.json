{
  "name": "fyrton",
  "version": "0.1.0",
  "private": true,
  "description": "Anna Cederberg-Orreteg official website",
  "homepage": "",
  "scripts": {
    "less:watch": "nodemon --watch src/styles --ext less --exec \"npm run less:build\"",
    "less:build": "lessc src/styles/main.less assets/styles.css",
    "eleventy:serve": "eleventy --serve",
    "dev": "npm-run-all --parallel less:watch eleventy:serve",
    "build": "npm run less:build && eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "ftp-deploy": "^2.4.7",
    "less": "^4.3.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "tslib": "^2.8.1"
  }
}
