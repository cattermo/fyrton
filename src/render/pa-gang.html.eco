<ul class="fyr_blog">
    <% for post in @getCollection('posts').toJSON().slice(0, 10): %>
    <li class="fyr_blog__item">
        <article>
            <span class="fyr_blog__date"><%= post.basename %></span>
            <%- post.contentRendered %>
        </article>
    </li>
    <% end %>
</ul>